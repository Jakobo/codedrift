{
  "strip": ["script"],
  "clobberPrefix": "user-content-",
  "clobber": ["name", "id", "rel"],
  "ancestors": {},
  "protocols": {
    "href": ["http", "https"],
    "cite": ["http", "https"],
    "src": ["http", "https"],
    "longDesc": ["http", "https"]
  },
  "tagNames": ["br", "a", "p"],
  "attributes": {
    "a": ["href", "rel"],
    "div": ["itemScope", "itemType"],
    "*": [
      "abbr",
      "accept",
      "acceptCharset",
      "accessKey",
      "ariaDescribedBy",
      "ariaHidden",
      "ariaLabel",
      "ariaLabelledBy",
      "dir",
      "lang",
      "itemProp"
    ]
  },
  "required": {
    "a": {
      "rel": "nofollow noopener"
    }
  }
}
